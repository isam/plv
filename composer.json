{
    "name": "plv/plv",
    "description": "Programming language version check tool",
    "keywords": ["cli", "programming language", "version"],
    "homepage": "https://github.com/isam/plv",
    "authors": [
       {
           "name": "Isamu Nagoya",
           "email": "nagodon@gmail.com"
       }
    ],
    "require": {
        "php": ">=5.3.3",
        "fabpot/goutte": "v1.0.1",
        "symfony/console": "v2.3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "Plv": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.0-dev"
        }
    }
}
